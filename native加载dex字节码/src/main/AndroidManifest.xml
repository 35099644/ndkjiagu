<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nativedex">

    <application
        android:name="com.example.nativedex.MyAppLication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true" android:theme="@style/AppTheme"
      >
        <meta-data android:name="APPLICATION_CLASS_NAME"
            android:value="com.example.dexx.MyApplication"/>

        <activity android:name="com.example.dexx.MainActivity"
            >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.dexx.MyTestActivity"/>
    </application>

</manifest>